<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="default">
  <PreFlow name="PreFlow">
    <Request/>
    <Response>
      <Step>
        <Name>add-cors</Name>
      </Step>
    </Response>
  </PreFlow>
  <Flows/>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <HTTPTargetConnection>
      <Authentication>
        <GoogleIDToken>
            <Audience ref="locationservice_url"></Audience>
        </GoogleIDToken>
      </Authentication>
      <URL>{locationservice_url}</URL>  
  </HTTPTargetConnection>
</TargetEndpoint>
